{
  "defaultSeverity": "error",
  "extends": [],
  "jsRules": {},
  "project": "./tsconfig.json",
  "rules": {
    "variable-name": false,
    "function-name": false,
    "jsx-no-lambda": false,
    "no-boolean-literal-compare": false,
    "interface-name": [true, "always-prefix"],
    "import-name": false,
    "no-any": true,
    "no-console": [true, "log", "error", "trace", "info"]
  },
  "rulesDirectory": []
}
